<odoo>
    <data>
        <report id="print_qr_codes" model="stock.quant.package" string="QR-codes" report_type="qweb-pdf" name="boxwise_wms.report_qr_codes" 
        file="boxwise_wms.report_qr_codes" menu="True" paperformat="boxwise_wms.paperformat_euro_landscape" />

        <template id="report_qr_codes">
            <t t-call="web.html_container">
            <t t-call="web.basic_layout">
                <div class="page">
                    <div class="row">
                        <t t-foreach="docs" t-as="doc">
                            <div class="col-xs-3 text-center">
                                <img width="200" height="200" t-att-src="'/report/barcode?type=QR&amp;value=%s' % ('https%3A%2F%2Fapp.boxwise.co%2Fqrcode%2Fpampiraiki%2F' + slug(doc) + '%2F')"/>
                                <p><span t-field="doc.name" /></p>
                            </div>
                        </t>
                    </div>
                </div>
            </t>
            </t>
        </template>
    </data>
</odoo>